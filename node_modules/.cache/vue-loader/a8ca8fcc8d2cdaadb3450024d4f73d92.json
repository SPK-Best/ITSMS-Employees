                        <v-icon color=\"red\"> {{ icons.mdiDelete }}</v-icon>\n                      </v-btn>\n\n                    </v-layout>\n                  </v-card>\n                </v-hover>\n              </v-col>\n            </template>\n          </v-slide-x-reverse-transition>\n        </v-row>\n      </v-container>\n    </v-form>\n  </v-app>\n</template>\n\n<script>\nimport db from '../fb.js';\nimport {mdiPencil, mdiDelete} from '@mdi/js'\n\nexport default {\n  name: \"Employees\",\n  data() {\n    return {\n      employee: {\n        name: '',\n        age: ''\n      },\n      allEmployees: [],\n      search: '',\n      icons: {\n        mdiPencil,\n        mdiDelete,\n      },\n      test: [1, 2, 3, 4, 5, 6],\n    }\n  },\n  methods: {\n    addEmployees() {\n      db.collection(\"Employee\")\n          .add(this.employee)\n          .then(() => this.read())\n          .catch((error) => {\n            console.error('Error removing document: ', error);\n          });\n      this.employee.name = '';\n      this.employee.age = '';\n    },\n    deleteEmployees(id, index) {\n      db.collection('Employee')\n          .doc(id)\n          .delete()\n          .then(() => {\n            console.log(\"Delete Complete\")\n          })\n          .catch((error) => {\n            console.error('Error removing document: ', error);\n          });\n      this.allEmployees.splice(index, 1);\n    },\n  },\n\n}\n</script>\n"]}]}